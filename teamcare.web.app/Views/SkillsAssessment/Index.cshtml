@{ ViewData["Title"] = "Skill Assessment";
    Layout = "~/Views/Shared/_Layout.cshtml";
    @model teamcare.web.app.ViewModels.SkillAssessmentViewModel
}
<div class="docs-content d-flex flex-column flex-column-fluid" id="kt_docs_content">
    <!--begin::Container-->
    <div class="container" id="kt_docs_content_container">
        <!--begin::Card-->
        <div class="card mb-2">

            <!--begin::Card Body-->
            <div class="card-body fs-6 py-15 px-10 py-lg-15 px-lg-15 text-gray-700">
                <div class="row g-9 mb-8">
                    <!--begin::Col-->
                    <div class="col-md-4 fv-row">
                    </div>
                    <div class="col-md-4 fv-row">
                      
                    </div>
                    <!--end::Col-->
                    <!--begin::Col-->
                    <div class="col-md-4 fv-row">
                        <label class="fs-6 fw-bold mb-2">Assessment Type</label>
                        <select class="form-select form-select-sm ddlselect2" name="assessmenttype" id="ddlassessmenttype">
                            @foreach (var group in Model.AssessmentType)
                            {
                                if (group.Id == Model.AssessmentTypeId)
                                {
                                    <option value="@group.Id" selected>@group.TypeName</option>
                                }
                                else
                                {
                                    <option value="@group.Id">@group.TypeName</option>
                                }
                            }
                        </select>
                    </div>
                    <!--end::Col-->
                </div>
                <div id="SkillGroupDataContent"></div>
            </div>
        </div>
    </div>
</div>

<!-- ------------------------------------- Model Portion ------------------------------------- -->
