@model teamcare.web.app.ViewModels.ServiceUsersViewModel

<div class="card mb-5 mb-xl-10">
    <div class="card-header cursor-pointer">
        <!--begin::Card title-->
        <div class="card-title m-0">
            <input type="hidden" id="hdnassettab" value="@(Model.AssessmentType.Where(r=>r.Position==0).Select(r=>r.Id).FirstOrDefault())" />
            <ul class="nav nav-stretch nav-line-tabs nav-line-tabs-2x border-transparent fs-5 fw-bolder flex-nowrap">
                <!--begin::Nav item-->
                @foreach (var item in Model.AssessmentType)
                {
                    
                    <li class="nav-item">
                        <a class="nav-link text-active-primary me-6 @(item.Position==0?"active":"")" data-bs-toggle="tab" id="@item.Id" href="#tab_@item.TypeName" onclick="setCurrentTabAssessment('@item.Id')">@item.TypeName</a>
                    </li>
                }
                <!--end::Nav item-->
            </ul>
        </div>
        <!--end::Card title-->
    </div>

    <div id="AssessmentTabContent">
    </div>
</div>